# Appwrite
APPWRITE_ENDPOINT=<YOUR_APPWRITE_ENDPOINT>
APPWRITE_PROJECT_ID=<YOUR_APPWRITE_PROJECT_ID>
APPWRITE_API_KEY=<YOUR_APPWRITE_API_KEY>
APPWRITE_DATABASE_ID=<YOUR_DATABASE_ID>
APPWRITE_USERS_COLLECTION_ID=users
OAUTH_CLIENTS_COLLECTION_ID=oauth_clients
OAUTH_TOKENS_COLLECTION_ID=oauth_tokens

# Express server
PORT=5000
OIDC_ISSUER=https://accounts.iymralabs.com
OIDC_COOKIE_KEYS=<LONG_RANDOM_KEY1>,<LONG_RANDOM_KEY2>
OIDC_JWKS_PATH=./server/secrets/jwks.json
OIDC_ACCESS_TTL=900
OIDC_REFRESH_TTL=604800

# Rate limiter
RATE_LIMITER_POINTS=60
RATE_LIMITER_DURATION=300

# JWT secret used only by v1 endpoints
JWT_SECRET=<YOUR_EXISTING_JWT_SECRET>